Proceso matriz
	fila1();
	fila2();
	fila3();
	fila4();
	
FinProceso

Subproceso fila1()
	
	Definir filas, columnas Como Entero;
	Definir matriz_1 Como caracter;
	Dimension matriz_1[1,5];
	matriz_1[0,0] <- "01";
	matriz_1[0,1] <- "02";
	matriz_1[0,2] <- "03";
	matriz_1[0,3] <- "04";
	matriz_1[0,4] <- "05";
	
	para filas<-0 hasta 0  con paso 1 hacer 
		para columnas <-0 hasta 4 con paso 1 hacer 
			escribir matriz_1[filas, columnas],"  |  ", sin saltar;
		FinPara
		Escribir "";
		
	FinPara
	
FinSubproceso

SubProceso fila2()
	Definir filas, columnas Como Entero;
	Definir matriz_1 Como caracter;
	Dimension matriz_1[1,5];
	matriz_1[0,0] <- "06";
	matriz_1[0,1] <- "07";
	matriz_1[0,2] <- "08";
	matriz_1[0,3] <- "09";
	matriz_1[0,4] <- "10";
	para filas<-0 hasta 0 con paso 1 hacer 
		para columnas <-4 hasta 0 con paso -1 hacer 
			escribir matriz_1[filas, columnas],"  |  ", sin saltar;
		FinPara
		Escribir "";
		
	FinPara
	
FinSubProceso

